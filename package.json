{
  "name": "ramapi",
  "version": "0.1.0",
  "description": "Ultra-fast, secure, and observable API framework for modern applications",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./middleware": {
      "import": "./dist/middleware/index.js",
      "types": "./dist/middleware/index.d.ts"
    },
    "./auth": {
      "import": "./dist/auth/index.js",
      "types": "./dist/auth/index.d.ts"
    },
    "./graphql": {
      "import": "./dist/protocols/graphql/index.js",
      "types": "./dist/protocols/graphql/index.d.ts"
    },
    "./grpc": {
      "import": "./dist/protocols/grpc/index.js",
      "types": "./dist/protocols/grpc/index.d.ts"
    }
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && tsc-alias",
    "build:protos": "npm run build && tsx example-app/multi-protocol/build-protos.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "example:rest": "tsx example-app/rest/server.ts",
    "example:graphql": "tsx example-app/graphql/server.ts",
    "example:grpc": "tsx example-app/grpc/server.ts",
    "example:multi": "tsx example-app/multi-protocol/server.ts",
    "example:multi:prod": "NODE_ENV=production npm run example:multi",
    "example:observability": "tsx example-app/observability/server.ts",
    "example:observability:prod": "NODE_ENV=production npm run example:observability",
    "example:profiling": "tsx example-app/profiling/server.ts",
    "example:profiling:prod": "NODE_ENV=production npm run example:profiling",
    "example:profiling:multi": "tsx example-app/profiling/multi-protocol.ts",
    "example:profiling:complete": "tsx example-app/profiling/complete-timeline.ts",
    "example:adapter:node": "tsx example-app/adapters/node-http-server.ts",
    "example:adapter:uws": "tsx example-app/adapters/uwebsockets-server.ts",
    "benchmark:adapters": "node benchmarks/adapter-comparison.js",
    "example": "npm run example:rest"
  },
  "keywords": [
    "api",
    "framework",
    "rest",
    "graphql",
    "grpc",
    "observability",
    "security",
    "typescript",
    "performance"
  ],
  "author": "Shanmukh Ram <bunny@syntaxsarcasm.world>",
  "repository": {
    "type": "git",
    "url": "https://github.com/shanmukhram/RamAPI.git"
  },
  "bugs": {
    "url": "https://github.com/shanmukhram/RamAPI/issues"
  },
  "homepage": "https://github.com/shanmukhram/RamAPI#readme",
  "license": "MIT",
  "dependencies": {
    "@grpc/grpc-js": "^1.9.14",
    "@grpc/proto-loader": "^0.7.10",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/core": "^2.1.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.206.0",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-trace-base": "^2.1.0",
    "@opentelemetry/sdk-trace-node": "^2.1.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "bcryptjs": "^2.4.3",
    "graphql": "^16.8.1",
    "jsonwebtoken": "^9.0.2",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#v20.48.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/google-protobuf": "^3.15.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.5",
    "@vitest/coverage-v8": "^1.2.1",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
